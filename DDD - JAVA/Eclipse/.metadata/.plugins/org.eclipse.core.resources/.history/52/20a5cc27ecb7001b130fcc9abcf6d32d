package challenge;

import java.util.Date;

public class Chat {

	Date data = new Date();
	
	private long chatId;
	private long userId;
	private String foto;
	private String msg;
	
	
	
	public Chat(long userId, long chatId, String msg) {
		this.chatId = chatId;
		this.userId = userId;
		this.msg = msg;
	}
	
	public long getChatId() {
		return chatId;
	}

	public long getUserId() {
		return userId;
	}

	public String getFoto() {
		return foto;
	}

	public String getMsg() {
		return msg;
	}

	public void testeData() {
		System.out.println(data);
	}
}
